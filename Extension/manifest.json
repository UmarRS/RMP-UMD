{
    "manifest_version": 1,
    "name": "Rate My Professor Extension Prototype",
    "version": "1.0",
    "description": "Append an icon next to the professor's name, which pops up a tab with their rate my professor info",
    "permissions": ["scripting"],
    "host_permissions": [
      "https://apps-banner-xe6-prod.its.umd.umich.edu:7005/StudentRegistrationSsb/ssb/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://apps-banner-xe6-prod.its.umd.umich.edu:7005/StudentRegistrationSsb/ssb/*"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["Icons/*"],
        "matches": ["<all_urls>"]
      }
    ],    
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "Icons/um16xtest.png",
      "48": "Icons/um48x.png",
      "128": "Icons/um128x.png"
    }
  }
  